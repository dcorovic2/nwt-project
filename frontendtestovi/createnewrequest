public class CreatenewrequestTest {
  private WebDriver driver;
  private Map<String, Object> vars;
  JavascriptExecutor js;
  @Before
  public void setUp() {
    driver = new ChromeDriver();
    js = (JavascriptExecutor) driver;
    vars = new HashMap<String, Object>();
  }
  @After
  public void tearDown() {
    driver.quit();
  }
  @Test
  public void createnewrequest() {
    driver.get("http://localhost:4200/");
    driver.manage().window().setSize(new Dimension(1373, 824));
    driver.findElement(By.cssSelector(".login-form")).click();
    driver.findElement(By.id("username")).sendKeys("idaa");
    driver.findElement(By.id("password")).sendKeys("ida");
    driver.findElement(By.id("password")).sendKeys(Keys.ENTER);
    driver.findElement(By.cssSelector(".fa-plus-circle")).click();
    driver.findElement(By.id("num")).click();
    driver.findElement(By.id("num")).click();
    driver.findElement(By.id("num")).sendKeys("3");
    driver.findElement(By.cssSelector(".leave-type > #\\#")).click();
    {
      WebElement dropdown = driver.findElement(By.cssSelector(".leave-type > #\\#"));
      dropdown.findElement(By.xpath("//option[. = 'Vacation']")).click();
    }
    driver.findElement(By.cssSelector(".leave-type > #\\#")).click();
    driver.findElement(By.id("comment")).click();
    driver.findElement(By.id("comment")).sendKeys("vacation");
    driver.findElement(By.cssSelector(".submit:nth-child(2) > span")).click();
    assertThat(driver.findElement(By.cssSelector(".ng-tns-c77-0:nth-child(2)")).getText(), is("You submitted request successfully!"));
  }
}
