public class JmbgemptyTest {
  private WebDriver driver;
  private Map<String, Object> vars;
  JavascriptExecutor js;
  @Before
  public void setUp() {
    driver = new ChromeDriver();
    js = (JavascriptExecutor) driver;
    vars = new HashMap<String, Object>();
  }
  @After
  public void tearDown() {
    driver.quit();
  }
  @Test
  public void jmbgempty() {
    driver.get("http://localhost:4200/");
    driver.manage().window().setSize(new Dimension(1536, 824));
    driver.findElement(By.cssSelector(".login-form")).click();
    driver.findElement(By.id("username")).sendKeys("admin");
    driver.findElement(By.id("password")).sendKeys("admin");
    driver.findElement(By.id("password")).sendKeys(Keys.ENTER);
    driver.findElement(By.cssSelector(".fa-bars")).click();
    driver.findElement(By.linkText("Requests")).click();
    driver.findElement(By.cssSelector(".fa-bars")).click();
    driver.findElement(By.cssSelector(".fa-user-plus")).click();
    driver.findElement(By.id("username")).sendKeys("stringuser");
    driver.findElement(By.id("pass")).sendKeys("stringpass");
    driver.findElement(By.id("firstname")).click();
    driver.findElement(By.id("firstname")).sendKeys("esma herenda");
    driver.findElement(By.id("jobrole")).click();
    driver.findElement(By.id("jobrole")).sendKeys("role");
    driver.findElement(By.id("email")).click();
    driver.findElement(By.id("email")).sendKeys("ems.herenda@gmail.com");
    driver.findElement(By.id("tel")).click();
    driver.findElement(By.id("tel")).sendKeys("062095994");
    driver.findElement(By.id("department")).click();
    driver.findElement(By.id("department")).sendKeys("SALES");
    driver.findElement(By.cssSelector(".settings-form > div:nth-child(2)")).click();
    driver.findElement(By.id("pass")).sendKeys("123");
    driver.findElement(By.id("confpass")).click();
    driver.findElement(By.id("confpass")).sendKeys("123");
    driver.findElement(By.id("alowance")).click();
    driver.findElement(By.id("alowance")).sendKeys("25");
    driver.findElement(By.cssSelector(".submit")).click();
    {
      WebElement element = driver.findElement(By.cssSelector(".submit"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).perform();
    }
    {
      WebElement element = driver.findElement(By.tagName("body"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element, 0, 0).perform();
    }
    assertThat(driver.findElement(By.cssSelector(".conf-new-class > .ng-star-inserted")).getText(), is("This field is incorrect!"));
  }
}
